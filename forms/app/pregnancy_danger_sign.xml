      <instance>
        <pregnancy_danger_sign id="pregnancy_danger_sign" prefix="J1!pregnancy_danger_sign!" delimiter="#" version="2022-10-25 15:12:13">
          <inputs>
            <meta>
              <location>
                <lat/>
                <long/>
                <error/>
                <message/>
              </location>
            </meta>
            <source>user</source>
            <source_id/>
            <contact>
              <_id/>
              <name/>
              <short_name/>
              <patient_id/>
              <date_of_birth>0</date_of_birth>
              <sex/>
              <parent>
                <_id/>
                <parent>
                  <contact>
                    <chw_name/>
                    <phone/>
                  </contact>
                </parent>
              </parent>
            </contact>
          </inputs>
          <patient_age_in_years tag="hidden"/>
          <patient_uuid tag="hidden"/>
          <patient_id tag="hidden"/>
          <patient_name tag="hidden"/>
          <patient_short_name tag="hidden"/>
          <patient_short_name_start tag="hidden"/>
          <t_danger_signs_referral_follow_up_date tag="hidden"/>
          <t_danger_signs_referral_follow_up tag="hidden"/>
          <pregnancy_uuid_ctx/>
          <danger_signs>
            <hf_examined/>
            <advice_note2/>
            <advice_note1/>
            <danger_signs_question_note/>
            <vaginal_bleeding/>
            <fits/>
            <severe_abdominal_pain/>
            <high_blood_pressure/>
            <very_pale/>
            <fever/>
            <breathlessness/>
            <child_heart_beat/>
            <face_hand_swelling/>
            <others/>
            <r_danger_sign_present tag="hidden"/>
            <congratulate_no_ds_note tag="hidden"/>
            <refer_patient_note_1 tag="hidden"/>
          </danger_signs>
          <data tag="hidden">
            <meta tag="hidden">
              <__patient_uuid/>
              <__patient_id/>
              <__household_uuid/>
              <__source/>
              <__source_id/>
              <__pregnancy_uuid/>
            </meta>
          </data>
          <meta tag="hidden">
            <instanceID/>
          </meta>
        </pregnancy_danger_sign>
      </instance>
      <instance id="contact-summary"/>
      <bind nodeset="/pregnancy_danger_sign/inputs" relevant="./source = 'user'"/>
      <bind nodeset="/pregnancy_danger_sign/inputs/source" type="string"/>
      <bind nodeset="/pregnancy_danger_sign/inputs/source_id" type="string"/>
      <bind nodeset="/pregnancy_danger_sign/inputs/contact/_id" type="db:person"/>
      <bind nodeset="/pregnancy_danger_sign/inputs/contact/name" type="string"/>
      <bind nodeset="/pregnancy_danger_sign/inputs/contact/short_name" type="string"/>
      <bind nodeset="/pregnancy_danger_sign/inputs/contact/patient_id" type="string"/>
      <bind nodeset="/pregnancy_danger_sign/inputs/contact/date_of_birth" type="string"/>
      <bind nodeset="/pregnancy_danger_sign/inputs/contact/sex" type="string"/>
      <bind nodeset="/pregnancy_danger_sign/inputs/contact/parent/_id" type="string"/>
      <bind nodeset="/pregnancy_danger_sign/inputs/contact/parent/parent/contact/chw_name" type="string"/>
      <bind nodeset="/pregnancy_danger_sign/inputs/contact/parent/parent/contact/phone" type="string"/>
      <bind nodeset="/pregnancy_danger_sign/patient_age_in_years" type="string" calculate="floor( difference-in-months( ../inputs/contact/date_of_birth, today() ) div 12 )"/>
      <bind nodeset="/pregnancy_danger_sign/patient_uuid" type="string" calculate="../inputs/contact/_id"/>
      <bind nodeset="/pregnancy_danger_sign/patient_id" type="string" calculate="../inputs/contact/patient_id"/>
      <bind nodeset="/pregnancy_danger_sign/patient_name" type="string" calculate="../inputs/contact/name"/>
      <bind nodeset="/pregnancy_danger_sign/patient_short_name" type="string" calculate="../inputs/contact/name"/>
      <bind nodeset="/pregnancy_danger_sign/patient_short_name_start" type="string" calculate="../inputs/contact/name"/>
      <bind nodeset="/pregnancy_danger_sign/t_danger_signs_referral_follow_up_date" type="string" calculate="date-time(floor(decimal-date-time(today())) + 3)"/>
      <bind nodeset="/pregnancy_danger_sign/t_danger_signs_referral_follow_up" type="string" calculate="../danger_signs/r_danger_sign_present"/>
      <bind nodeset="/pregnancy_danger_sign/pregnancy_uuid_ctx" type="string" calculate="if(instance('contact-summary')/context/pregnancy_uuid != '', instance('contact-summary')/context/pregnancy_uuid, .)"/>
      <bind nodeset="/pregnancy_danger_sign/danger_signs/hf_examined" type="select1" required="true()"/>
      <bind nodeset="/pregnancy_danger_sign/danger_signs/advice_note2" readonly="true()" type="string" relevant=" /pregnancy_danger_sign/danger_signs/hf_examined = 'yes'"/>
      <bind nodeset="/pregnancy_danger_sign/danger_signs/advice_note1" readonly="true()" type="string" relevant=" /pregnancy_danger_sign/danger_signs/hf_examined = 'no'"/>
      <bind nodeset="/pregnancy_danger_sign/danger_signs/danger_signs_question_note" readonly="true()" type="string"/>
      <bind nodeset="/pregnancy_danger_sign/danger_signs/vaginal_bleeding" type="select1" required="true()"/>
      <bind nodeset="/pregnancy_danger_sign/danger_signs/fits" type="select1" required="true()"/>
      <bind nodeset="/pregnancy_danger_sign/danger_signs/severe_abdominal_pain" type="select1" required="true()"/>
      <bind nodeset="/pregnancy_danger_sign/danger_signs/high_blood_pressure" type="select1" required="true()"/>
      <bind nodeset="/pregnancy_danger_sign/danger_signs/very_pale" type="select1" required="true()"/>
      <bind nodeset="/pregnancy_danger_sign/danger_signs/fever" type="select1" required="true()"/>
      <bind nodeset="/pregnancy_danger_sign/danger_signs/breathlessness" type="select1" required="true()"/>
      <bind nodeset="/pregnancy_danger_sign/danger_signs/child_heart_beat" type="select1" required="true()"/>
      <bind nodeset="/pregnancy_danger_sign/danger_signs/face_hand_swelling" type="select1" required="true()"/>
      <bind nodeset="/pregnancy_danger_sign/danger_signs/others" type="string"/>
      <bind nodeset="/pregnancy_danger_sign/danger_signs/r_danger_sign_present" type="string" calculate="if(selected(../vaginal_bleeding, 'yes') 
 or selected(../fits, 'yes') 
 or selected(../severe_abdominal_pain, 'yes') 
 or selected(../high_blood_pressure, 'yes')
 or selected(../very_pale, 'yes')
 or selected(../fever, 'yes')
 or selected(../face_hand_swelling, 'yes')
 or selected(../child_heart_beat, 'no')
 or selected(../breathlessness, 'yes'), 'yes',
 if(selected(../vaginal_bleeding, 'no')
 and selected(../fits, 'no')
 and selected(../severe_abdominal_pain, 'no')
 and selected(../high_blood_pressure, 'no')
 and selected(../very_pale, 'no')
 and selected(../fever, 'no')
 and selected(../face_hand_swelling, 'no')
 and selected(../child_heart_beat, 'yes')
 and selected(../breathlessness, 'no'), 'no', ''))"/>
      <bind nodeset="/pregnancy_danger_sign/danger_signs/congratulate_no_ds_note" readonly="true()" type="string" relevant="../r_danger_sign_present = 'no'"/>
      <bind nodeset="/pregnancy_danger_sign/danger_signs/refer_patient_note_1" readonly="true()" type="string" relevant="../r_danger_sign_present = 'yes'"/>
      <bind nodeset="/pregnancy_danger_sign/data/meta/__patient_uuid" type="string" calculate="../../../inputs/contact/_id"/>
      <bind nodeset="/pregnancy_danger_sign/data/meta/__patient_id" type="string" calculate="../../../inputs/contact/patient_id"/>
      <bind nodeset="/pregnancy_danger_sign/data/meta/__household_uuid" type="string" calculate="../../../inputs/contact/parent/_id"/>
