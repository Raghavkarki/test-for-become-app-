      <instance>
        <non_communicable_diseases id="non_communicable_diseases" prefix="J1!non_communicable_diseases!" delimiter="#" version="16-08-2021 12-01-17">
          <inputs>
            <meta>
              <location>
                <lat/>
                <long/>
                <error/>
                <message/>
              </location>
            </meta>
            <source>user</source>
            <source_id/>
            <contact>
              <_id/>
              <name/>
              <short_name/>
              <patient_id/>
              <date_of_birth>0</date_of_birth>
              <sex/>
              <parent>
                <_id/>
                <parent>
                  <contact>
                    <chw_name/>
                    <phone/>
                  </contact>
                </parent>
              </parent>
            </contact>
          </inputs>
          <patient_age_in_years tag="hidden"/>
          <patient_uuid tag="hidden"/>
          <patient_id tag="hidden"/>
          <patient_name tag="hidden"/>
          <non_communicable_diseases>
            <heart_disease/>
            <high_blood_pressure/>
            <diabetes/>
            <copd/>
            <cancer/>
            <kidney_disease/>
            <thyroid/>
            <congenital_health/>
            <has_other_ncd/>
            <other_ncd/>
            <taking_medicine/>
            <not_taking_medicine/>
            <other_reason/>
          </non_communicable_diseases>
          <data tag="hidden">
            <meta tag="hidden">
              <__patient_uuid/>
              <__patient_id/>
              <__household_uuid/>
              <__source/>
              <__source_id/>
            </meta>
          </data>
          <meta tag="hidden">
            <instanceID/>
          </meta>
        </non_communicable_diseases>
      </instance>
      <instance id="contact-summary"/>
      <bind nodeset="/non_communicable_diseases/inputs" relevant="./source = 'user'"/>
      <bind nodeset="/non_communicable_diseases/inputs/source" type="string"/>
      <bind nodeset="/non_communicable_diseases/inputs/source_id" type="string"/>
      <bind nodeset="/non_communicable_diseases/inputs/contact/_id" type="db:person"/>
      <bind nodeset="/non_communicable_diseases/inputs/contact/name" type="string"/>
      <bind nodeset="/non_communicable_diseases/inputs/contact/short_name" type="string"/>
      <bind nodeset="/non_communicable_diseases/inputs/contact/patient_id" type="string"/>
      <bind nodeset="/non_communicable_diseases/inputs/contact/date_of_birth" type="string"/>
      <bind nodeset="/non_communicable_diseases/inputs/contact/sex" type="string"/>
      <bind nodeset="/non_communicable_diseases/inputs/contact/parent/_id" type="string"/>
      <bind nodeset="/non_communicable_diseases/inputs/contact/parent/parent/contact/chw_name" type="string"/>
      <bind nodeset="/non_communicable_diseases/inputs/contact/parent/parent/contact/phone" type="string"/>
      <bind nodeset="/non_communicable_diseases/patient_age_in_years" type="string" calculate="floor( difference-in-months( ../inputs/contact/date_of_birth, today() ) div 12 )"/>
      <bind nodeset="/non_communicable_diseases/patient_uuid" type="string" calculate="../inputs/contact/_id"/>
      <bind nodeset="/non_communicable_diseases/patient_id" type="string" calculate="../inputs/contact/patient_id"/>
      <bind nodeset="/non_communicable_diseases/patient_name" type="string" calculate="../inputs/contact/name"/>
      <bind nodeset="/non_communicable_diseases/non_communicable_diseases/heart_disease" type="select1" required="true()"/>
      <bind nodeset="/non_communicable_diseases/non_communicable_diseases/high_blood_pressure" type="select1" required="true()"/>
      <bind nodeset="/non_communicable_diseases/non_communicable_diseases/diabetes" type="select1" required="true()"/>
      <bind nodeset="/non_communicable_diseases/non_communicable_diseases/copd" type="select1" required="true()"/>
      <bind nodeset="/non_communicable_diseases/non_communicable_diseases/cancer" type="select1" required="true()"/>
      <bind nodeset="/non_communicable_diseases/non_communicable_diseases/kidney_disease" type="select1" required="true()"/>
      <bind nodeset="/non_communicable_diseases/non_communicable_diseases/thyroid" type="select1" required="true()"/>
      <bind nodeset="/non_communicable_diseases/non_communicable_diseases/congenital_health" type="select1" required="true()"/>
      <bind nodeset="/non_communicable_diseases/non_communicable_diseases/has_other_ncd" type="select1" required="true()"/>
      <bind nodeset="/non_communicable_diseases/non_communicable_diseases/other_ncd" type="string" jr:constraintMsg="jr:itext('/non_communicable_diseases/non_communicable_diseases/other_ncd:jr:constraintMsg')" constraint="string-length(.) &lt;= 300" relevant=" /non_communicable_diseases/non_communicable_diseases/has_other_ncd ='yes'" required="true()"/>
      <bind nodeset="/non_communicable_diseases/non_communicable_diseases/taking_medicine" type="select1" relevant=" /non_communicable_diseases/non_communicable_diseases/heart_disease ='yes' 
or  /non_communicable_diseases/non_communicable_diseases/high_blood_pressure ='yes'
or  /non_communicable_diseases/non_communicable_diseases/diabetes ='yes'
or  /non_communicable_diseases/non_communicable_diseases/copd ='yes'
or  /non_communicable_diseases/non_communicable_diseases/cancer ='yes'
or  /non_communicable_diseases/non_communicable_diseases/kidney_disease ='yes'
or  /non_communicable_diseases/non_communicable_diseases/thyroid ='yes'
or  /non_communicable_diseases/non_communicable_diseases/congenital_health ='yes'
or  /non_communicable_diseases/non_communicable_diseases/has_other_ncd ='yes'" required="true()"/>
      <bind nodeset="/non_communicable_diseases/non_communicable_diseases/not_taking_medicine" type="select1" relevant=" /non_communicable_diseases/non_communicable_diseases/taking_medicine ='no' or  /non_communicable_diseases/non_communicable_diseases/taking_medicine  = 'yes_some'" required="true()"/>
      <bind nodeset="/non_communicable_diseases/non_communicable_diseases/other_reason" type="string" jr:constraintMsg="jr:itext('/non_communicable_diseases/non_communicable_diseases/other_reason:jr:constraintMsg')" constraint="string-length(.) &lt;= 300" relevant=" /non_communicable_diseases/non_communicable_diseases/not_taking_medicine ='other'" required="true()"/>
      <bind nodeset="/non_communicable_diseases/data/meta/__patient_uuid" type="string" calculate="../../../inputs/contact/_id"/>
      <bind nodeset="/non_communicable_diseases/data/meta/__patient_id" type="string" calculate="../../../inputs/contact/patient_id"/>
      <bind nodeset="/non_communicable_diseases/data/meta/__household_uuid" type="string" calculate="../../../inputs/contact/parent/_id"/>
      <bind nodeset="/non_communicable_diseases/data/meta/__source" type="string" calculate="../../../inputs/source"/>
      <bind nodeset="/non_communicable_diseases/data/meta/__source_id" type="string" calculate="../../../inputs/source_id"/>
      <bind nodeset="/non_communicable_diseases/meta/instanceID" type="string" readonly="true()" calculate="concat('uuid:', uuid())"/>
