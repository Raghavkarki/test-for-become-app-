      </itext>
      <instance>
        <hypertension_referral id="hypertension_referral" prefix="J1!hypertension_referral!" delimiter="#" version="1">
          <inputs>
            <meta>
              <location>
                <lat/>
                <long/>
                <error/>
                <message/>
              </location>
            </meta>
            <source>user</source>
            <source_id/>
            <contact>
              <_id/>
              <name/>
              <patient_id/>
            </contact>
          </inputs>
          <patient_uuid/>
          <patient_name/>
          <patient_id/>
          <danger_sign_code tag="hidden"/>
          <bmical_show_ctx tag="hidden"/>
          <bmi_result_ctx tag="hidden"/>
          <systolic_result tag="hidden"/>
          <daistolic_result tag="hidden"/>
          <avg_result tag="hidden"/>
          <follow_up>
            <visit_method/>
          </follow_up>
          <hypertension_referral_form>
            <generated_note_name_28 tag="hidden"/>
            <generated_note_name_29 tag="hidden"/>
            <generated_note_name_30 tag="hidden"/>
            <generated_note_name_31 tag="hidden"/>
            <danger_sign tag="hidden"/>
            <danger_sign1 tag="hidden"/>
            <danger_sign2 tag="hidden"/>
            <danger_sign3 tag="hidden"/>
            <danger_sign4 tag="hidden"/>
            <danger_sign5 tag="hidden"/>
            <visit_healthfacilities/>
            <multiple_diseases/>
            <specify_diseases/>
            <generated_note_name_41 tag="hidden"/>
            <not_visit/>
            <other_reason/>
            <generated_note_name_44 tag="hidden"/>
          </hypertension_referral_form>
          <meta tag="hidden">
            <instanceID/>
          </meta>
        </hypertension_referral>
      </instance>
      <instance id="contact-summary"/>
      <bind nodeset="/hypertension_referral/inputs" relevant="./source = 'user'"/>
      <bind nodeset="/hypertension_referral/inputs/source" type="string"/>
      <bind nodeset="/hypertension_referral/inputs/source_id" type="string"/>
      <bind nodeset="/hypertension_referral/inputs/contact/_id" type="db:person"/>
      <bind nodeset="/hypertension_referral/inputs/contact/name" type="string"/>
      <bind nodeset="/hypertension_referral/inputs/contact/patient_id" type="string"/>
      <bind nodeset="/hypertension_referral/patient_uuid" type="string" calculate="../inputs/contact/_id"/>
      <bind nodeset="/hypertension_referral/patient_name" type="string" calculate="../inputs/contact/name"/>
      <bind nodeset="/hypertension_referral/patient_id" type="string" calculate="../inputs/contact/_id"/>
      <bind nodeset="/hypertension_referral/danger_sign_code" type="string"/>
      <bind nodeset="/hypertension_referral/bmical_show_ctx" type="string" calculate="if(instance('contact-summary')/context/bmical_show_ctx, instance('contact-summary')/context/bmical_show_ctx, .)"/>
      <bind nodeset="/hypertension_referral/bmi_result_ctx" type="string" calculate="if(instance('contact-summary')/context/bmi_result_ctx, instance('contact-summary')/context/bmi_result_ctx, .)"/>
      <bind nodeset="/hypertension_referral/systolic_result" type="string" calculate="if(instance('contact-summary')/context/systolic_result, instance('contact-summary')/context/systolic_result, .)"/>
      <bind nodeset="/hypertension_referral/daistolic_result" type="string" calculate="if(instance('contact-summary')/context/diastolic_result, instance('contact-summary')/context/diastolic_result, .)"/>
      <bind nodeset="/hypertension_referral/avg_result" type="string" calculate="if(instance('contact-summary')/context/avg_result, instance('contact-summary')/context/avg_result, .)"/>
      <bind nodeset="/hypertension_referral/follow_up/visit_method" type="select1" required="true()"/>
      <bind nodeset="/hypertension_referral/hypertension_referral_form/generated_note_name_28" readonly="true()" type="string"/>
      <bind nodeset="/hypertension_referral/hypertension_referral_form/generated_note_name_29" readonly="true()" type="string"/>
      <bind nodeset="/hypertension_referral/hypertension_referral_form/generated_note_name_30" readonly="true()" type="string"/>
      <bind nodeset="/hypertension_referral/hypertension_referral_form/generated_note_name_31" readonly="true()" type="string"/>
      <bind nodeset="/hypertension_referral/hypertension_referral_form/danger_sign" readonly="true()" type="string"/>
      <bind nodeset="/hypertension_referral/hypertension_referral_form/danger_sign1" readonly="true()" type="string" relevant="contains( /hypertension_referral/danger_sign_code , 'eye_vision')"/>
      <bind nodeset="/hypertension_referral/hypertension_referral_form/danger_sign2" readonly="true()" type="string" relevant="contains( /hypertension_referral/danger_sign_code , 'head_ache')"/>
      <bind nodeset="/hypertension_referral/hypertension_referral_form/danger_sign3" readonly="true()" type="string" relevant="contains( /hypertension_referral/danger_sign_code , 'chest_pain')"/>
      <bind nodeset="/hypertension_referral/hypertension_referral_form/danger_sign4" readonly="true()" type="string" relevant="contains( /hypertension_referral/danger_sign_code , 'tire_breath')"/>
      <bind nodeset="/hypertension_referral/hypertension_referral_form/danger_sign5" readonly="true()" type="string" relevant="contains( /hypertension_referral/danger_sign_code , 'swollen_part')"/>
      <bind nodeset="/hypertension_referral/hypertension_referral_form/visit_healthfacilities" type="select1" required="true()"/>
      <bind nodeset="/hypertension_referral/hypertension_referral_form/multiple_diseases" type="select" required="true()" relevant=" /hypertension_referral/hypertension_referral_form/visit_healthfacilities  = 'yes'"/>
      <bind nodeset="/hypertension_referral/hypertension_referral_form/specify_diseases" type="string" required="true()" jr:constraintMsg="max characters = 100" relevant="selected( /hypertension_referral/hypertension_referral_form/multiple_diseases , 'other') and count-selected( /hypertension_referral/hypertension_referral_form/multiple_diseases ) &gt;= 1" constraint="string-length(.)&lt;=100"/>
      <bind nodeset="/hypertension_referral/hypertension_referral_form/generated_note_name_41" readonly="true()" type="string" relevant=" /hypertension_referral/hypertension_referral_form/visit_healthfacilities  = 'yes'"/>
      <bind nodeset="/hypertension_referral/hypertension_referral_form/not_visit" type="select1" required="true()" relevant=" /hypertension_referral/hypertension_referral_form/visit_healthfacilities  = 'no'"/>
      <bind nodeset="/hypertension_referral/hypertension_referral_form/other_reason" type="string" required="true()" jr:constraintMsg="max characters = 100" relevant=" /hypertension_referral/hypertension_referral_form/not_visit  ='otherreason'" constraint="string-length(.)&lt;=100"/>
      <bind nodeset="/hypertension_referral/hypertension_referral_form/generated_note_name_44" readonly="true()" type="string" relevant=" /hypertension_referral/hypertension_referral_form/visit_healthfacilities  = 'no'"/>
      <bind nodeset="/hypertension_referral/meta/instanceID" type="string" readonly="true()" calculate="concat('uuid:', uuid())"/>
    </model>
  </h:head>
  <h:body class="pages">
    <group appearance="field-list" ref="/hypertension_referral/inputs">
      <label ref="jr:itext('/hypertension_referral/inputs:label')"/>
      <group ref="/hypertension_referral/inputs/contact">
        <label ref="jr:itext('/hypertension_referral/inputs/contact:label')"/>
        <input appearance="db-object" ref="/hypertension_referral/inputs/contact/_id"/>
      </group>
    </group>
    <group appearance="field-list" ref="/hypertension_referral/follow_up">
      <select1 ref="/hypertension_referral/follow_up/visit_method">
        <label ref="jr:itext('/hypertension_referral/follow_up/visit_method:label')"/>
        <item>
          <label ref="jr:itext('/hypertension_referral/follow_up/visit_method/person:label')"/>
          <value>person</value>
        </item>
        <item>
          <label ref="jr:itext('/hypertension_referral/follow_up/visit_method/phone:label')"/>
