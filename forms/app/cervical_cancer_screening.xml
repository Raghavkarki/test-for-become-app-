      <bind nodeset="/cervical_cancer_screening/screening_for_cervical_cancer/cervical_cancer_details/result" required="true()" type="select1"/>
      <bind nodeset="/cervical_cancer_screening/screening_for_cervical_cancer/further_health_facility" relevant=" /cervical_cancer_screening/screening_for_cervical_cancer/cervical_cancer_details/result  = 'positive' or  /cervical_cancer_screening/screening_for_cervical_cancer/cervical_cancer_details/result  = 'suspicious'" required="true()" type="select1"/>
