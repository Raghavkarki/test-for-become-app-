      <bind nodeset="/cervical_cancer_screening/inputs/contact/_id" type="db:person"/>
      <bind nodeset="/cervical_cancer_screening/inputs/contact/patient_id" type="string"/>
      <bind nodeset="/cervical_cancer_screening/inputs/contact/name" type="string"/>
      <bind calculate="../inputs/contact/_id" nodeset="/cervical_cancer_screening/patient_uuid" type="string"/>
