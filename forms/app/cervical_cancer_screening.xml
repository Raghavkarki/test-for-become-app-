      <bind nodeset="/cervical_cancer_screening/screening_for_cervical_cancer/cervical_cancer_discussion" readonly="true()" type="string"/>
      <bind nodeset="/cervical_cancer_screening/screening_for_cervical_cancer/heard_about_cervical_cancer" required="true()" type="select1"/>
