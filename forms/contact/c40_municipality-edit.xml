          <label ref="jr:itext('/data/c40_municipality/change_primary/yes:label')"/>
          <value>yes</value>
        </item>
        <item>
          <label ref="jr:itext('/data/c40_municipality/change_primary/no:label')"/>
          <value>no</value>
        </item>
      </select1>
      <group ref="/data/c40_municipality/contact">
        <input appearance="select-contact type-c12_center_contact" ref="/data/c40_municipality/contact/select_center_contact">
          <label ref="jr:itext('/data/c40_municipality/contact/select_center_contact:label')"/>
        </input>
        <input appearance="select-contact type-c22_province_contact" ref="/data/c40_municipality/contact/select_province_contact">
          <label ref="jr:itext('/data/c40_municipality/contact/select_province_contact:label')"/>
        </input>
        <input appearance="select-contact type-c32_district_contact" ref="/data/c40_municipality/contact/select_district_contact">
          <label ref="jr:itext('/data/c40_municipality/contact/select_district_contact:label')"/>
        </input>
        <input appearance="select-contact type-c42_municipality_contact" ref="/data/c40_municipality/contact/select_municipality_contact">
          <label ref="jr:itext('/data/c40_municipality/contact/select_municipality_contact:label')"/>
        </input>
        <input appearance="select-contact type-c52_ward_contact" ref="/data/c40_municipality/contact/select_ward_contact">
          <label ref="jr:itext('/data/c40_municipality/contact/select_ward_contact:label')"/>
        </input>
        <input appearance="select-contact type-c62_chn_area_contact" ref="/data/c40_municipality/contact/select_chn_area_contact">
          <label ref="jr:itext('/data/c40_municipality/contact/select_chn_area_contact:label')"/>
        </input>
        <input appearance="select-contact type-c72_fchv_area_contact" ref="/data/c40_municipality/contact/select_fchv_area_contact">
          <label ref="jr:itext('/data/c40_municipality/contact/select_fchv_area_contact:label')"/>
