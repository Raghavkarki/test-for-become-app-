      <instance>
        <data id="contact:c40_municipality:edit" prefix="J1!contact:c40_municipality:edit!" delimiter="#" version="2021-12-12 17-18">
          <inputs>
            <meta>
              <location>
                <lat/>
                <long/>
                <error/>
                <message/>
              </location>
            </meta>
            <user>
              <contact_id/>
              <facility_id/>
              <name/>
            </user>
          </inputs>
          <c40_municipality>
            <parent/>
            <type/>
            <contact_type/>
            <name/>
            <external_id/>
            <notes/>
            <place_type/>
            <place_type_translation/>
            <contact_name_note/>
            <change_primary>no</change_primary>
            <contact>
              <name/>
              <contact_name/>
              <select_center_contact/>
              <select_province_contact/>
              <select_district_contact/>
              <select_municipality_contact/>
              <select_ward_contact/>
              <select_chn_area_contact/>
              <select_fchv_area_contact/>
              <_id/>
            </contact>
          </c40_municipality>
          <meta tag="hidden">
            <instanceID/>
          </meta>
        </data>
      </instance>
      <bind nodeset="/data/inputs/user" relevant="false()"/>
      <bind nodeset="/data/inputs/user/contact_id" type="string"/>
      <bind nodeset="/data/inputs/user/facility_id" type="string"/>
      <bind nodeset="/data/inputs/user/name" type="string"/>
      <bind nodeset="/data/c40_municipality/parent" type="string" readonly="true()"/>
      <bind nodeset="/data/c40_municipality/type" type="string" readonly="true()"/>
      <bind nodeset="/data/c40_municipality/contact_type" type="string" readonly="true()"/>
      <bind nodeset="/data/c40_municipality/name" type="string"/>
      <bind nodeset="/data/c40_municipality/external_id" type="string"/>
      <bind nodeset="/data/c40_municipality/notes" type="string"/>
      <bind nodeset="/data/c40_municipality/place_type" type="select1" calculate="&quot;c40_municipality&quot;"/>
      <bind nodeset="/data/c40_municipality/place_type_translation" type="string" calculate="jr:choice-name( /data/c40_municipality/place_type ,' /data/c40_municipality/place_type ')"/>
      <bind nodeset="/data/c40_municipality/contact_name_note" readonly="true()" type="string"/>
      <bind nodeset="/data/c40_municipality/change_primary" type="select1" required="true()"/>
      <bind nodeset="/data/c40_municipality/contact/name" type="string" readonly="true()"/>
      <bind nodeset="/data/c40_municipality/contact/contact_name" type="string" calculate="../name"/>
      <bind nodeset="/data/c40_municipality/contact/select_center_contact" type="string" relevant=" /data/c40_municipality/change_primary  = 'yes' and  /data/c40_municipality/place_type  = 'c10_center'" required="true()"/>
      <bind nodeset="/data/c40_municipality/contact/select_province_contact" type="string" relevant=" /data/c40_municipality/change_primary  = 'yes' and  /data/c40_municipality/place_type  = 'c20_province'" required="true()"/>
      <bind nodeset="/data/c40_municipality/contact/select_district_contact" type="string" relevant=" /data/c40_municipality/change_primary  = 'yes' and  /data/c40_municipality/place_type  = 'c30_district'" required="true()"/>
      <bind nodeset="/data/c40_municipality/contact/select_municipality_contact" type="string" relevant=" /data/c40_municipality/change_primary  = 'yes' and  /data/c40_municipality/place_type  = 'c40_municipality'" required="true()"/>
      <bind nodeset="/data/c40_municipality/contact/select_ward_contact" type="string" relevant=" /data/c40_municipality/change_primary  = 'yes' and  /data/c40_municipality/place_type  = 'c50_ward'" required="true()"/>
      <bind nodeset="/data/c40_municipality/contact/select_chn_area_contact" type="string" relevant=" /data/c40_municipality/change_primary  = 'yes' and  /data/c40_municipality/place_type  = 'c60_chn_area'" required="true()"/>
      <bind nodeset="/data/c40_municipality/contact/select_fchv_area_contact" type="string" relevant=" /data/c40_municipality/change_primary  = 'yes' and  /data/c40_municipality/place_type  = 'c70fchv_area'" required="true()"/>
      <bind nodeset="/data/c40_municipality/contact/_id" type="string" calculate="if( /data/c40_municipality/change_primary  = 'yes', coalesce(../select_center_contact, ../select_province_contact, ../select_district_contact, ../select_municipality_contact, ../select_ward_contact, ../select_chn_area_contact, ../select_fchv_area_contact), .)"/>
      <bind nodeset="/data/meta/instanceID" type="string" readonly="true()" calculate="concat('uuid:', uuid())"/>
    </model>
  </h:head>
  <h:body>
    <group ref="/data/inputs">
