            <generated_name>c70_fchv_area</generated_name>
            <generated_name_translation/>
            <person_type>c70_fchv_area</person_type>
          </init>
          <contact>
            <parent>PARENT</parent>
            <type>contact</type>
            <contact_type/>
            <role/>
            <first_name/>
            <last_name/>
            <name/>
            <age/>
            <gender>female</gender>
            <caste_code/>
            <phone>+977</phone>
            <fchv_id_number/>
            <start_date_as_fchv/>
            <start_date_as_fchv_ad/>
            <basic_training/>
            <post/>
            <meta tag="hidden">
              <created_by/>
              <created_by_person_uuid/>
              <created_by_place_uuid/>
            </meta>
          </contact>
          <meta tag="hidden">
            <instanceID/>
          </meta>
        </data>
      </instance>
      <bind nodeset="/data/inputs" relevant="false()"/>
      <bind nodeset="/data/inputs/user/contact_id" type="string"/>
      <bind nodeset="/data/inputs/user/facility_id" type="string"/>
      <bind nodeset="/data/inputs/user/name" type="string"/>
      <bind nodeset="/data/init/create_new_person" type="select1" required="true()"/>
      <bind nodeset="/data/init/name" type="string"/>
      <bind nodeset="/data/init/contact_name" type="string" calculate="coalesce(../name, ../../contact/name)"/>
      <bind nodeset="/data/init/custom_place_name_label_translator" type="select1" relevant="selected( /data/init/create_new_person ,'none')" calculate="&quot;name&quot;"/>
      <bind nodeset="/data/init/custom_place_name_label" type="string" calculate="jr:choice-name( /data/init/custom_place_name_label_translator ,' /data/init/custom_place_name_label_translator ')"/>
      <bind nodeset="/data/init/custom_place_name" type="string" relevant="not( /data/init/place_type  = 'c60_chn_area' or  /data/init/place_type  = 'c70_fchv_area') or selected( /data/c70_fchv_area/is_name_generated , 'false') or selected( /data/init/create_new_person ,'none')" required="true()"/>
      <bind nodeset="/data/init/place_type" type="select1"/>
      <bind nodeset="/data/init/place_type_translation" type="string" calculate="jr:choice-name( /data/init/place_type ,' /data/init/place_type ')"/>
      <bind nodeset="/data/init/generated_name" type="select1"/>
      <bind nodeset="/data/init/generated_name_translation" type="string" calculate="jr:choice-name( /data/init/generated_name ,' /data/init/generated_name ')"/>
      <bind nodeset="/data/init/person_type" type="select1"/>
      <bind nodeset="/data/contact" relevant="selected( /data/init/create_new_person ,'new_person')"/>
      <bind nodeset="/data/contact/parent" type="string"/>
      <bind nodeset="/data/contact/type" type="string"/>
      <bind nodeset="/data/contact/contact_type" type="string" calculate="jr:choice-name( /data/init/person_type ,' /data/init/person_type ')"/>
      <bind nodeset="/data/contact/role" type="string" calculate="if( /data/init/place_type  = 'c70_fchv_area', 'chw', 'app_user')"/>
      <bind nodeset="/data/contact/first_name" type="string" constraint="string-length(.)&lt;=30 and regex(.,'^([^0-9]*)$')" jr:constraintMsg="jr:itext('/data/contact/first_name:jr:constraintMsg')" required="true()"/>
      <bind nodeset="/data/contact/last_name" type="string" constraint="string-length(.)&lt;=30 and regex(.,'^([^0-9]*)$')" jr:constraintMsg="jr:itext('/data/contact/last_name:jr:constraintMsg')" required="true()"/>
      <bind nodeset="/data/contact/name" type="string" calculate="concat( /data/contact/first_name , ' ',  /data/contact/last_name )"/>
      <bind nodeset="/data/contact/age" type="int"/>
      <bind nodeset="/data/contact/gender" type="select1" required="true()"/>
      <bind nodeset="/data/contact/caste_code" type="select" constraint="not(selected(., 'unknown') and (selected(., 'dalit') or selected(., 'janajati') or selected(., 'madhesi')or selected(., 'muslim')or selected(., 'brahmin_chettri') or selected(., 'other') ))" jr:constraintMsg="jr:itext('/data/contact/caste_code:jr:constraintMsg')" relevant=" /data/contact/role  = 'chw'" required="true()"/>
      <bind nodeset="/data/contact/phone" type="tel" constraint="true()" jr:constraintMsg="jr:itext('/data/contact/phone:jr:constraintMsg')" required="true()"/>
      <bind nodeset="/data/contact/fchv_id_number" type="string" constraint="regex(.,'^[0-9]*$')" jr:constraintMsg="jr:itext('/data/contact/fchv_id_number:jr:constraintMsg')" relevant=" /data/contact/role  = 'chw'"/>
      <bind nodeset="/data/contact/start_date_as_fchv" type="string" constraint="regex(.,'^[0-9]{4}$') and int(.)&gt;= 2044 and int(format-date(today(),'%Y')) + 57 &gt;= int(.)" jr:constraintMsg="jr:itext('/data/contact/start_date_as_fchv:jr:constraintMsg')" relevant=" /data/contact/role  = 'chw'" required="true()"/>
      <bind nodeset="/data/contact/start_date_as_fchv_ad" type="string" relevant=" /data/contact/role  = 'chw'" calculate="int( /data/contact/start_date_as_fchv ) - 57"/>
      <bind nodeset="/data/contact/basic_training" type="select1" relevant=" /data/contact/role  = 'chw'" required="true()"/>
      <bind nodeset="/data/contact/post" type="select1" relevant=" /data/contact/role  = 'app_user'"/>
      <bind nodeset="/data/contact/meta/created_by" type="string" calculate="../../../inputs/user/name"/>
      <bind nodeset="/data/contact/meta/created_by_person_uuid" type="string" calculate="../../../inputs/user/contact_id"/>
      <bind nodeset="/data/contact/meta/created_by_place_uuid" type="string" calculate="../../../inputs/user/facility_id"/>
      <bind nodeset="/data/c70_fchv_area/parent" type="string"/>
      <bind nodeset="/data/c70_fchv_area/type" type="string"/>
      <bind nodeset="/data/c70_fchv_area/is_name_generated" type="select1" relevant="not(selected( /data/init/create_new_person ,'none')) and ( /data/init/place_type  = 'c60_chn_area' or  /data/init/place_type  = 'c70_fchv_area')" required="true()"/>
      <bind nodeset="/data/c70_fchv_area/name" type="string" required="true()" calculate="if(  /data/c70_fchv_area/is_name_generated  = 'true',  /data/init/generated_name_translation ,  /data/init/custom_place_name )"/>
      <bind nodeset="/data/c70_fchv_area/external_id" type="string"/>
      <bind nodeset="/data/c70_fchv_area/notes" type="string"/>
      <bind nodeset="/data/c70_fchv_area/contact" type="string" calculate="if(selected( /data/init/create_new_person ,'none'), &quot;&quot;, &quot;NEW&quot;)"/>
      <bind nodeset="/data/c70_fchv_area/geolocation" type="string" calculate="concat(../../inputs/meta/location/lat, concat(' ', ../../inputs/meta/location/long))"/>
      <bind nodeset="/data/c70_fchv_area/meta/created_by" type="string" calculate="../../../inputs/user/name"/>
      <bind nodeset="/data/c70_fchv_area/meta/created_by_person_uuid" type="string" calculate="../../../inputs/user/contact_id"/>
      <bind nodeset="/data/c70_fchv_area/meta/created_by_place_uuid" type="string" calculate="../../../inputs/user/facility_id"/>
      <bind nodeset="/data/meta/instanceID" type="string" readonly="true()" calculate="concat('uuid:', uuid())"/>
    </model>
  </h:head>
  <h:body class="pages">
    <group ref="/data/inputs">
