      </itext>
      <instance>
        <data id="contact:c22_province_contact:create" prefix="J1!contact:c22_province_contact:create!" delimiter="#" version="2020-01-27">
          <c22_province_contact>
            <parent>PARENT</parent>
            <first_name/>
            <last_name/>
            <name/>
            <gender/>
            <age/>
            <phone>+977</phone>
            <post/>
            <post_other/>
          </c22_province_contact>
          <meta tag="hidden">
            <instanceID/>
          </meta>
        </data>
      </instance>
      <bind nodeset="/data/c22_province_contact/parent" type="string"/>
      <bind nodeset="/data/c22_province_contact/first_name" type="string" constraint="string-length(.)&lt;=30 and regex(.,'^([^0-9]*)$')" jr:constraintMsg="jr:itext('/data/c22_province_contact/first_name:jr:constraintMsg')" required="true()"/>
      <bind nodeset="/data/c22_province_contact/last_name" type="string" constraint="string-length(.)&lt;=30 and regex(.,'^([^0-9]*)$')" jr:constraintMsg="jr:itext('/data/c22_province_contact/last_name:jr:constraintMsg')" required="true()"/>
      <bind nodeset="/data/c22_province_contact/name" type="string" calculate="concat( /data/c22_province_contact/first_name , ' ',  /data/c22_province_contact/last_name )"/>
      <bind nodeset="/data/c22_province_contact/gender" type="select1" required="true()"/>
      <bind nodeset="/data/c22_province_contact/age" type="int" constraint=".&lt;=100" jr:constraintMsg="jr:itext('/data/c22_province_contact/age:jr:constraintMsg')" required="true()"/>
      <bind nodeset="/data/c22_province_contact/phone" type="tel" jr:constraintMsg="jr:itext('/data/c22_province_contact/phone:jr:constraintMsg')" required="true()"/>
      <bind nodeset="/data/c22_province_contact/post" type="select1" required="true()"/>
      <bind nodeset="/data/c22_province_contact/post_other" type="string" relevant=" /data/c22_province_contact/post  = 'other'"/>
      <bind nodeset="/data/meta/instanceID" type="string" readonly="true()" calculate="concat('uuid:', uuid())"/>
    </model>
  </h:head>
  <h:body class="pages">
    <group appearance="field-list" ref="/data/c22_province_contact">
      <input ref="/data/c22_province_contact/first_name">
        <label ref="jr:itext('/data/c22_province_contact/first_name:label')"/>
        <hint ref="jr:itext('/data/c22_province_contact/first_name:hint')"/>
      </input>
      <input ref="/data/c22_province_contact/last_name">
        <label ref="jr:itext('/data/c22_province_contact/last_name:label')"/>
      </input>
      <select1 ref="/data/c22_province_contact/gender">
        <label ref="jr:itext('/data/c22_province_contact/gender:label')"/>
        <item>
          <label ref="jr:itext('/data/c22_province_contact/gender/female:label')"/>
          <value>female</value>
        </item>
        <item>
          <label ref="jr:itext('/data/c22_province_contact/gender/male:label')"/>
          <value>male</value>
        </item>
        <item>
          <label ref="jr:itext('/data/c22_province_contact/gender/other:label')"/>
          <value>other</value>
        </item>
      </select1>
      <input ref="/data/c22_province_contact/age">
        <label ref="jr:itext('/data/c22_province_contact/age:label')"/>
      </input>
      <input ref="/data/c22_province_contact/phone">
        <label ref="jr:itext('/data/c22_province_contact/phone:label')"/>
        <hint ref="jr:itext('/data/c22_province_contact/phone:hint')"/>
      </input>
      <select1 appearance="horizontal" ref="/data/c22_province_contact/post">
        <label ref="jr:itext('/data/c22_province_contact/post:label')"/>
        <item>
          <label ref="jr:itext('/data/c22_province_contact/post/pho:label')"/>
          <value>pho</value>
        </item>
        <item>
          <label ref="jr:itext('/data/c22_province_contact/post/pha:label')"/>
          <value>pha</value>
        </item>
        <item>
          <label ref="jr:itext('/data/c22_province_contact/post/other:label')"/>
          <value>other</value>
        </item>
      </select1>
      <input ref="/data/c22_province_contact/post_other">
        <label ref="jr:itext('/data/c22_province_contact/post_other:label')"/>
      </input>
    </group>
  </h:body>
</h:html>
